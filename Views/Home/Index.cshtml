                            <div class="mb-4">
                                @Html.TextBoxFor(model => model.Name, new { @class = "form-control border-0 py-3", placeholder = "Your Name" })
                                @Html.ValidationMessageFor(model => model.Name, "", new { @class = "text-danger" })
                            </div>

                            <div class="mb-4">
                                @Html.TextBoxFor(model => model.Email, new { @class = "form-control border-0 py-3", placeholder = "Your Email" })
                                @Html.ValidationMessageFor(model => model.Email, "", new { @class = "text-danger" })
                            </div>

                            <div class="mb-4">
                                @Html.TextBoxFor(model => model.Project, new { @class = "form-control border-0 py-3", placeholder = "Project" })
                                @Html.ValidationMessageFor(model => model.Project, "", new { @class = "text-danger" })
                            </div>

